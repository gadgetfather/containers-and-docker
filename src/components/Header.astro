---
import { Icon } from 'astro-icon';
const {} = Astro.props
const TEXT = {
    title: "Containers And Docker"
}
---

  <header class="bg-gray-100 py-4 px-8">
    <div class="container mx-auto flex items-center justify-between">
        <a href="/">
            <h1 class="text-2xl font-bold">{TEXT.title}</h1>
        </a>
        <div class="flex gap-4">
          <a class="hover:underline" href="https://docs.google.com/presentation/d/14FBfhplm2UJxzFdX131x15Fd51LJAWWst5l7hi646_o/edit?usp=sharing">Slides</a>
          <a href="https://github.com/gadgetfather/containers-and-docker" target="_blank"><Icon class="h-5 w-5" name="mdi:github"></Icon></a>
        </div>
    </div>
  </header>

